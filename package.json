		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.16.2",
		"sass": "^1.63.6",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
